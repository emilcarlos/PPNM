set terminal svg
set key bottom right
set output "erf.svg"
set xlabel "x"
set ylabel "y"
set tics out
set xzeroaxis
set yzeroaxis
set samples 800
set title "Error function"
plot [-5:5][-1.5:1.5] \
 "erf.data" with lines linetype 1 title "theory" \
,"table.data" with points pointtype 4 title "experiment" \
